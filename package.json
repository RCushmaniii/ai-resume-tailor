{
  "name": "ai-resume-tailor",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter ./client dev",
    "dev:client": "pnpm --filter ./client dev",
    "dev:server": "cd server && python app.py",
    "build": "pnpm --filter ./client build",
    "preview": "pnpm --filter ./client preview",
    "lint": "pnpm --filter ./client lint",
    "lint:fix": "pnpm --filter ./client lint:fix",
    "typecheck": "pnpm --filter ./client typecheck",
    "format": "prettier --write .",
    "verify": "pnpm --filter ./client verify",
    "install:client": "pnpm --filter ./client install",
    "install:server": "cd server && python -m venv venv && .\\venv\\Scripts\\activate && pip install -r requirements.txt && python -m spacy download en_core_web_sm"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.8"
  }
}